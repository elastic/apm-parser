{
  "name": "@elastic/performance-testing-dataset-extractor",
  "version": "0.0.1",
  "description": "A library to convert APM traces into JSON format for performance testing.",
  "author": "Cavit Baturalp Gurdin <baturalp.gurdin@elastic.co>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/apm-parser.git"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "performance-testing-dataset-extractor": "dist/esbuild/cli.js"
  },
  "scripts": {
    "main": "ts-node src/index.ts",
    "cli": "ts-node src/cli.ts",
    "lint": "npx eslint . --ext .ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package output",
    "docs": "typedoc --entryPoints src/index.ts",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build && yarn esbuild-node",
    "esbuild-node": "esbuild src/cli.ts --bundle --platform=node --outfile=dist/esbuild/cli.js",
    "esbuild-node:dev": "esbuild src/cli.ts --bundle --platform=node --sourcemap=external --outfile=dist/esbuild/cli.js",
    "esbuild-node:watch": "esbuild src/cli.ts --bundle --platform=node --watch --sourcemap=external --outfile=dist/esbuild/cli.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.17.0",
    "axios": "^0.26.1",
    "axios-curlirize": "1.3.7",
    "lodash": "^4.17.21",
    "qs": "^6.10.3",
    "tslib": "^2.3.1",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@types/axios-curlirize": "^1.3.2",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.180",
    "@types/node": "^15.0.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "esbuild": "^0.11.11",
    "eslint": "^8.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.6.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/elastic/apm-parser/issues"
  },
  "homepage": "https://github.com/elastic/apm-parser#readme",
  "directories": {
    "doc": "docs",
    "test": "tests"
  }
}
